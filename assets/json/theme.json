{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"custom": true,
			"link": true
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1440px"
		},
		"typography": {
			"dropCap": false,
			"customFontSize": true,
			"fontSizes": [
				{
					"slug": "x-small",
					"size": "0.625rem",
					"name": "Extra Small:  "
				},
				{
					"slug": "small",
					"size": "0.75",
					"name": "Small:  "
				},
				{
					"slug": "medium",
					"size": "1rem",
					"name": "Medium:  "
				},
				{
					"slug": "large",
					"size": "1.375rem",
					"name": "Large:  "
				},
				{
					"slug": "super",
					"size": "2rem",
					"name": "Super:  "
				},
				{
					"slug": "heading-1",
					"size": "5.625rem",
					"name": "Heading 1:  "
				},
				{
					"slug": "heading-2",
					"size": "4.375rem",
					"name": "Heading 2:  "
				},
                {
					"slug": "heading-3",
					"size": "3.125rem",
					"name": "Heading 3:  "
				},
				{
					"slug": "sub-heading",
					"size": "2.25rem",
					"name": "Sub-heading:  "
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,Ubuntu,Lato,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Ubuntu\", serif",
					"name": "Ubuntu",
					"slug": "ubuntu",
					"fontFace": [
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:../fonts/ubuntu-v20-latin-300italic.woff2" ]
						},
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:../fonts/ubuntu-v20-latin-regular.woff2" ]
						},
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:../fonts/ubuntu-v20-latin-italic.woff2" ]
						},
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/ubuntu-v20-latin-500.woff2" ]
						},
						{
							"fontFamily": "Ubuntu",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:../fonts/ubuntu-v20-latin-700.woff2" ]
						}
					]
				}
			]
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true,
			"units": ["px", "em", "rem", "vh", "vw"]
		}
	},
	"styles": {
		"elements": {
			"link": {
				"color": {
					"text": "#D6724E"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu)",
					"fontSize": "clamp( 2.5rem, calc(1.5vw + 1rem), 2.8rem )",
					"lineHeight": "clamp( 2.5rem, calc(1.5vw + 1rem), 2.8rem )",
					"fontWeight": "500"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "1.3rem",
						"left": "0",
						"right": "0"
					}
				},
				"color": {
					"text": "#333333"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu)",
					"fontSize": "clamp( 1.875rem, calc(1.5vw + 1rem), 2rem )",
					"lineHeight": "clamp( 1.875rem, calc(1.5vw + 1rem), 2rem )",
					"fontWeight": "500"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "1.3rem",
						"left": "0",
						"right": "0"
					}
				},
				"color": {
					"text": "#333333"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu)",
					"fontSize": "clamp( 1.5rem, calc(1.5vw + 1rem), 1.8rem )",
					"lineHeight": "clamp( 1.5rem, calc(1.5vw + 1rem), 1.8rem )",
					"fontWeight": "500"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "1.3rem",
						"left": "0",
						"right": "0"
					}
				},
				"color": {
					"text": "#333333"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu)",
					"fontSize": "clamp( 1.25rem, calc(1.5vw + 1rem), 1.5rem )",
					"lineHeight": "clamp( 1.25rem, calc(1.5vw + 1rem), 1.5rem )",
					"fontWeight": "500"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "1.3rem",
						"left": "0",
						"right": "0"
					}
				},
				"color": {
					"text": "#333333"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu)",
					"fontSize": "clamp( 1.125rem, calc(1.5vw + 1rem), 1.3rem )",
					"lineHeight": "clamp( 1.125rem, calc(1.5vw + 1rem), 1.3rem )",
					"fontWeight": "500"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "1.3rem",
						"left": "0",
						"right": "0"
					}
				},
				"color": {
					"text": "#333333"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ubuntu)",
					"fontSize": "clamp( 1rem, calc(1.5vw + 1rem), 1.2rem )",
					"lineHeight": "clamp( 1rem, calc(1.5vw + 1rem), 1.2rem )",
					"fontWeight": "500"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "1.3rem",
						"left": "0",
						"right": "0"
					}
				},
				"color": {
					"text": "#333333"
				}
			}
		},
		"spacing":{
			"padding": {
				"top": "0",
				"bottom": "0",
				"left": "0",
				"right": "0"
			},
			"margin": {
				"top": "0",
				"bottom": "0",
				"left": "0",
				"right": "0"
			},
			"blockGap": "1.2rem"
		},
		"border": {
			"radius": "4px"
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"typography": {
			"fontSize": "22.5px",
			"lineHeight": "40px",
			"fontWeight": "300"
		},
		"blocks": {
			"core/paragraph": {
				"spacing":{
					"padding": {
						"top": "0",
						"bottom": "0",
						"left": "0",
						"right": "0"
					},
					"margin": {
						"top": "0",
						"bottom": "0",
						"left": "0",
						"right": "0"
					}
				},
				"typography": {
					"fontSize": "clamp( 1.6rem, calc(1.5vw + 1rem), 1.8rem )",
					"lineHeight": "clamp( 1.8rem, calc(1.5vw + 1rem), 2rem )",
					"fontWeight": "400"
				}
			},
			"core/pullquote": {
				"color": {
					"text": "#D26A48",
					"background": "#D9D7CD !important"
				},
				"spacing": {
					"margin": {
						"top": "1em",
						"bottom": "1em",
						"left": "0",
						"right": "0"
					},
					"padding": {
						"top": "clamp( 17px, calc(1.25vw + 1rem), 24px )",
						"bottom": "clamp( 17px, calc(1.25vw + 1rem), 24px )",
						"left": "clamp( 16px, calc(1vw + 1rem), 20px )",
						"right": "clamp( 16px, calc(1vw + 1rem), 20px )"
					}
				}
			},
			"core/quote": {
				"color": {
					"text": "#D26A48",
					"background": "#D9D7CD !important"
				},
				"spacing": {
					"margin": {
						"top": "1em",
						"bottom": "1em",
						"left": "0",
						"right": "0"
					},
					"padding": {
						"top": "clamp( 17px, calc(1.25vw + 1rem), 24px )",
						"bottom": "clamp( 17px, calc(1.25vw + 1rem), 24px )",
						"left": "clamp( 16px, calc(1vw + 1rem), 20px )",
						"right": "clamp( 16px, calc(1vw + 1rem), 20px )"
					}
				}
			},
			"core/button": {
				"spacing": {
					"padding": {
						"top": "18px",
						"bottom": "18px",
						"right": "20px",
						"left": "20px"
					},
					"margin": {
						"top": "20px",
						"bottom": "20px"
					}
				},
				"typography": {
					"fontSize": "18px",
					"fontFamily": "Raleway, sans-serif",
					"lineHeight": "16px"
				}
			}
		}
	}
}
